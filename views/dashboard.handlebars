<div class="container">
    <div class="row">
        <div class="col-md-8">
            {{#each posts}}
            <a class="list-item flex-row justify-space-between" href="/dashboard/edit/{{id}}">
                <span>{{title}}</span>
                <span>{{formatDate createdAt}}</span>
            </a>
            {{/each}}
        </div>
    </div>
</div>